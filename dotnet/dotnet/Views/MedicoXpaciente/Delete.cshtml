@model dotnet.Models.DB.MedicoXpaciente

@{
    ViewData["Title"] = "Delete";
}


<h1>Eliminar Asignación</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdMedicoNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdMedicoNavigation.Documento)
        </dd class>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdPacienteNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdPacienteNavigation.Documento)
        </dd class>
    </dl>
    
     <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        
         <a class="btn btn-outline-primary" asp-action="Index">Cancelar</a>
         <input type="submit" value="Delete" class="btn btn-danger" />
    </form>
</div>
