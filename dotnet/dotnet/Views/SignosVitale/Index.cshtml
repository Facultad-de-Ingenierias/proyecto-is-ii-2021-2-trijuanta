@model IEnumerable<dotnet.Models.DB.SignosVitale>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Oximetria)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FrecCardiaca)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FrecRespiratoria)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Temperatura)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PresionArterial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Glicemias)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdEnfermeroNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Oximetria)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FrecCardiaca)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FrecRespiratoria)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Temperatura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PresionArterial)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Glicemias)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdEnfermeroNavigation.Celular)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
